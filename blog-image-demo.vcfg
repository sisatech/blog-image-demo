info:
  name: Vorteil Blog Demo
  author: Sisa-Tech Pty Ltd
  description: A simple application which processes image files provided and outputs an abstract version of each image.
  version: 1.0
env:
  vars:
    DEMO_SOURCE_DIR: pictures
    DEMO_PROCESSED_DIR: processed
    DEMO_PLACEHOLDER_PICTURE: pictures/vorteil.png
network:
  nics:
  - ip: dhcp
  ports:
  - port: 8080
    protocol: http
